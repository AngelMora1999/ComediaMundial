<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @channel.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= @channel.description %>
</p>

<p>
  <strong>Visit count:</strong>
  <%= @channel.visit_count %>
</p>

<p>
  <h1>Mis videos:</h1>
  <% @channel.videos.each do |video| %>
  	<%= link_to "#{video.title}", video %>
  <% end %>
</p>

<%= link_to 'Edit', edit_channel_path(@channel) %> |
<%= link_to 'Back', channels_path %>
